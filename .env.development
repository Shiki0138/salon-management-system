# Development Environment Variables

# Database
DATABASE_URL="postgresql://salon_user:salon_password@localhost:5432/salon_management"

# Redis
REDIS_URL="redis://localhost:6379"

# Application
NODE_ENV="development"
PORT=4002
API_VERSION="v1"

# JWT Configuration (Development - OK to use default)
JWT_SECRET="development-jwt-secret-key-not-for-production-use"
JWT_REFRESH_SECRET="development-refresh-secret-key-not-for-production-use"
ACCESS_TOKEN_EXPIRY="15m"
REFRESH_TOKEN_EXPIRY="7d"

# Session Configuration (Development - OK to use default)
SESSION_SECRET="development-session-secret-key-not-for-production-use"

# Security Configuration
RATE_LIMIT_WINDOW_MS="900000"
RATE_LIMIT_MAX_REQUESTS="1000"
TRUST_PROXY="false"
ENABLE_CORS="true"

# Logging
LOG_LEVEL="debug"

# Development features
ENABLE_DEBUG="true"
HOT_RELOAD="true"